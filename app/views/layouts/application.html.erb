<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= render "layouts/stylesheets" %>
  <%= javascript_include_tag :all %>
  <%= csrf_meta_tag %>
  <%= yield(:head) %>
</head>
<body>
  <div class="container">
    <%= render "layouts/header" %><br />
    <section class="round">
      <% flash.each do |key, value| %>
        <%= content_tag(:div, value, :class => "flash #{key}") %>
      <% end %>
      <%= yield %>
    </section>
    <%= render "layouts/footer" %>
    <%= debug(params) if Rails.env.development? %>  </div>
</body>
</html>
